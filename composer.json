{
  "name": "aflexon/promocode-system",
  "type": "project",
  "description": "Promocode system",
  "require": {
    "php": "^8.2",
    "ramsey/uuid": "^4.7"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "phpunit/phpunit": "^10"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
